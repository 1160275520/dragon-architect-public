#!/bin/bash

if [ $# -lt 3 ]; then
    echo Usage: `basename $0` gamename cid "[-D|-P]" 1>&2
    echo "Invokes init, markcid gamename cid, transfer -D/-P, clean."
    exit 1
fi

DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
PROG="$DIR/cgsanalysis"

$PROG init && $PROG markcid $1 $2 && $PROG transfer $3 && $PROG clean

